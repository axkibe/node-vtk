+
+static int isBlacklisted(
+  const char *name
+)
+{
+  char const * const *e;
+
+#ifndef DO_WHITELIST
+  for (e = blacklist; *e; e++)
+    {
+    if (!strcmp(*e, name) ) return 1;
+    }
+  return 0;
+#else
+  for (e = whitelist; *e; e++)
+    {
+    if (!strcmp(*e, name) ) return 0;
+    }
+  return 1;
+#endif
+}
