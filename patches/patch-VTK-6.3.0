+/* -------------------------------------------------------------------- */
+/* Returns true if there is no lowercase char in s.       */
+
+static int isUpperString(
+  char const *s
+)
+{
+  while (*s)
+    {
+    if (*s != toupper(*s)) return 0;
+    s++;
+    }
+
+  return 1;
+}
