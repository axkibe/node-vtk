+/* -------------------------------------------------------------------- */
+/* TODO */
+
+static int parameterEqualInOverload(
+  ValueInfo *p1info,
+  ValueInfo *p2info
+)
+{
+  if( p1info->Type == p2info->Type ) return 1;
+
+  if( p1info->Type != VTK_PARSE_OBJECT ) return 0;
+
+  return !strcmp(p1info->Class, p2info->Class );
+}
